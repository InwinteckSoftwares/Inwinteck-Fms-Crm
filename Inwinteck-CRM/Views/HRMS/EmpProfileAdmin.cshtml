                                                    <div class="col-md-6" id="Marriage_Date">
                                                        <div class="form-group">
                                                            <label>Marriage Date</label>
                                                            <input class="form-control" name="Marriage_Date" type="date">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="submit-section">
