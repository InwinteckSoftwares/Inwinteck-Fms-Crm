@model HRMS.Models.Employee_Profile
@{
    ViewBag.Title = "EmpProfileAdmin";
    Layout = ViewBag.Layout;
}

<style>
    #Marriage_Date {
        display: none;
    }
</style>

<div class="card mb-0">
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <div class="profile-view">
                    <div class="profile-img-wrap">
                        <div class="profile-img">
                            <a href="#" data-target="#profile_info" data-toggle="modal"><img alt="" src="@Model.ED.Photo"></a>
                        </div>
                    </div>
                    <div class="profile-basic">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="profile-info-left">
                                    <h3 class="user-name m-t-0 mb-0">@Model.ED.First_Name @Model.ED.Last_Name</h3>
                                    @*<h6 class="text-muted">HR Team</h6>
                                        <small class="text-muted">HR Manager</small>*@
                                    <div class="staff-id">Employee ID : @Model.ED.Emp_Code</div>
                                    <div class="small doj text-muted">Date of Join : @Model.ED.DOJ.Value.ToString("dd-MM-yyyy")</div>

                                </div>
                            </div>
                            <div class="col-md-7">
                                <ul class="personal-info">
                                    <li>
                                        <div class="title">Phone:</div>
                                        @if (Model.ED.Phone == null)
                                        {
                                            <div class="text"><a href="#" data-target="#profile_info" data-toggle="modal">Kindly Add your Phone Number</a></div>
                                        }
                                        else
                                        {
                                            <div class="text"><a href="#" data-target="#profile_info" data-toggle="modal">@Model.ED.Phone</a></div>

                                        }
                                    </li>
                                    <li>
                                        <div class="title">Email:</div>
                                        <div class="text"><a href="#" data-target="#profile_info" data-toggle="modal">@Model.ED.Email</a></div>
                                    </li>
                                    <li>
                                        <div class="title">Designation:</div>
                                        <div class="text">@Model.ED.Designation</div>
                                    </li>
                                    <li>
                                        <div class="title">Department:</div>
                                        <div class="text">@Model.ED.Department</div>
                                    </li>
                                    <li>
                                        <div class="title">Gender:</div>
                                        <div class="text">@Model.ED.Gender</div>
                                    </li>
                                    <li>
                                        <div class="title">Reports to:</div>
                                        <div class="text">
                                            <div class="avatar-box">
                                                <div class="avatar avatar-xs">
                                                    <img src="@Model.ED.rmPhoto" alt="">
                                                </div>
                                            </div>
                                            <a href="#" data-target="#profile_info" data-toggle="modal">
                                                @Model.ED.Reporting_Manager
                                            </a>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="pro-edit"><a data-target="#profile_info" data-toggle="modal" class="edit-icon" href="#"><i class="fas fa-pencil-alt"></i></a></div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="tab-content">

    <div class="card tab-box mt-3">
        <div class="row user-tabs">
            <div class="col-lg-12 col-md-12 col-sm-12 line-tabs">
                <ul class="nav nav-tabs nav-tabs-bottom pt-3 pb-2">
                    <li class="nav-item"><a href="#emp_profile" data-toggle="tab" class="nav-link active">Profile</a></li>
                    <li class="nav-item"><a href="#bank_statutory" data-toggle="tab" class="nav-link">Bank & Salary <small class="text-danger">(Admin Only)</small></a></li>
                </ul>
            </div>
        </div>
    </div>
    <!-- Profile Info Tab -->
    <div id="emp_profile" class="pro-overview tab-pane fade show active">
